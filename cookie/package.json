{
  "name": "cookie",
  "version": "1.0.0",
  "description": "understand how to use cookie",
  "main": "index.js",
  "scripts": {
    "tsbuild": "webpack --config webpack.config.js",
    "test": "mocha-webpack --webpack-config webpack.config.test.js ",
    "css": "npm-run-all css-compile css-prefix css-minify copy-img",
    "css-compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 css/config/index.scss dist/css/custom.css",
    "css-prefix": "postcss --config css/config/postcss.config.js --replace dist/css/*.css",
    "css-minify": "cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/custom.min.css dist/css/custom.css",
    "copy-img": "cpx \"./img/{*.png,*gif}\" ./dist/img",
    "copy-html": "cpx \"./html/**\" ./dist/html",
    "server": "tsc ts/server/app.ts",
    "db": "tsc ts/server/mongoapp.ts"
  },
  "keywords": [
    "cookie",
    "typescript"
  ],
  "author": "au",
  "license": "ISC",
  "dependencies": {
    "@types/c3": "^0.6.0",
    "@types/mocha": "^5.0.0",
    "@types/mongodb": "^3.0.10",
    "@types/node": "^9.6.2",
    "c3": "^0.5.2",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "Server": "^1.0.0",
    "assert": "^1.4.1",
    "autoprefixer": "^8.2.0",
    "bootstrap": "^4.0.0",
    "chai": "^4.1.2",
    "clean-css-cli": "^4.1.11",
    "cpx": "^1.5.0",
    "http": "0.0.0",
    "http-server": "^0.11.1",
    "mocha": "^5.0.5",
    "mocha-webpack": "^1.1.0",
    "mongodb": "^3.0.5",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0",
    "server": "^1.0.18",
    "ts-loader": "^4.1.0",
    "typings": "^2.1.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  }
}
